{
	"name": "@igor.dvlpr/zep",
	"version": "5.0.0",
	"description": "🧠 Zep is a zero-dependency, efficient debounce module. ⏰",
	"type": "module",
	"engines": {
		"node": ">=12.17"
	},
	"sideEffects": false,
	"main": "main",
	"exports": {
		".": {
			"require": "./dist/compat/main.cjs",
			"import": "./dist/module/main.js"
		}
	},
	"module": "./dist/module/main.js",
	"types": "./dist/main",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf ./dist/*",
		"build:cjs": "cross-env BABEL_ENV=cjs babel src -d dist/compat --out-file-extension .cjs",
		"build:esm": "cross-env BABEL_ENV=esm babel src -d dist/module",
		"build:dts": "tsc",
		"build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:dts",
		"test": "mocha --timeout 4000",
		"pub": "npm run build && npm test && npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/igorskyflyer/npm-zep.git"
	},
	"keywords": [
		"zep",
		"timeout",
		"interval",
		"debounce",
		"throttle",
		"timer",
		"igor.dvlpr",
		"igorskyflyer",
		"utility"
	],
	"author": "Igor Dimitrijević",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/igorskyflyer/npm-zep/issues"
	},
	"homepage": "https://github.com/igorskyflyer/npm-zep#readme",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/plugin-transform-modules-commonjs": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@types/chai": "^4.2.21",
		"@types/mocha": "^9.0.0",
		"@types/node": "^12.17.0",
		"babel-plugin-add-import-extension": "^1.6.0",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"mocha": "^9.0.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.5"
	}
}
